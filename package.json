{
  "name": "package-tls",
  "version": "1.3.0",
  "description": "package-tls 提供了解析 npm 的包配置文件 package.json 的相关工具",
  "main": "dist/package-tls.cjs.js",
  "module": "dist/package-tls.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "homepage": "https://github.com/GuoBinyong/package-tls#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuoBinyong/package-tls"
  },
  "keywords": [
    "npm",
    "package",
    "package.json",
    "tools",
    "npm tools",
    "package tools",
    "解析 工具"
  ],
  "author": {
    "name": "郭斌勇",
    "email": "guobinyong@qq.com"
  },
  "license": "MIT",
  "files": [
    "dist/[^.]*",
    "types/[^.]*",
    "docs/[^.]*",
    "doc/[^.]*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "bunch": "vite build --mode bunch",
    "preview": "vite preview",
